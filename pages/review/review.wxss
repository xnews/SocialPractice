  right: 20rpx;
  bottom: 20rpx;
}
.practiceDescContainer{
  padding: 20rpx 10rpx;
}
.choosePracItem text{
  margin-right: 40rpx;
}
.batchExportFeedBack{
  font-weight:400;
  white-space:nowrap;
