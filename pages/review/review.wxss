.bthAcManaget {
  margin-top: 100rpx;
}
.unitManagementBtn{
  margin-top: 160rpx;
}
.specifiedItem{
  display: flex;
  align-items: center;
  margin: 25rpx 10rpx;
}
.specifiedItem textarea{
  margin-left: 40rpx;
  width: 70%;
  height: 150rpx;
  border: 1rpx solid #cccccc;
